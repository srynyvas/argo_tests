{
    "kind": "SeldonDeployment",
    "apiVersion": "{{ .Values.apiVersion}}",
    "metadata": {
        "name": "{{ .Release.Name }}",
        "namespace": "argo-test-apps"
    },
    "spec":{
        "name": "{{ .Release.Name }}",
        "protocol": "{{ .Values.protocol }}"

    },
    "predictors": [
        {
            "componentSpecs": [
                {
                    "graph": {
                        "children": [],
                        "endpoint": {
                            "type": "REST"
                        },
                        "implementation": "{{ .Values.model.implementation}}",
                        "modelUri": "{{ .Values.model.modelUri}}",
                        "name": "classifier"
                    },
                    "replicas": 1
                }
            ]

    }]
}